{
  "name": "node-starter",
  "version": "0.0.0",
  "scripts": {
    "css-w": "npx tailwindcss -i ./src/style.css -o ./public/compiled.style.css -w",
    "css-w:2": "npx tailwindcss -i ./src/style2.css -o ./public/compiled.style2.css -w",
